        "import Hapi from '@hapi/hapi';\r\n\r\nconst start = async() => {\r\n    const server = Hapi.server({\r\n        port: 8000,\r\n        host: 'localhost'\r\n    });\r\n\r\n\r\n    server.route({\r\n        method: 'GET',\r\n        path: '/hello',\r\n        handler: (req, h) => {\r\n            return h.response('Hello Client response!').code(201);\r\n        }\r\n    })\r\n\r\n    await server.start();\r\n    console.log(`Server is listening on ${server.info.uri}`)\r\n}\r\n\r\nprocess.on('unhandledRejection', err => {\r\n    console.log(err);\r\n    process.exit(1);\r\n})\r\n\r\nstart();"
